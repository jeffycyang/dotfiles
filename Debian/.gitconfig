[user]
    name = Jeff Y.C. Yang
    email = YOUR_EMAIL@example.com

[init]
    defaultBranch = main

[core]
    editor = code --wait
    excludesfile = ~/.gitignore_global

[pull]
    rebase = false

[push]
    autoSetupRemote = true

[alias]
    co = checkout
    br = branch
    ci = commit
    st = status
    lg = log --oneline --graph --decorate --all
    last = log -1 HEAD
    unstage = reset HEAD --
